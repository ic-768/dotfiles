{
  "explorer.icon.enableNerdfont":false,
  "explorer.width": 50,
  "explorer.previewAction.onHover":false,
  "explorer.keyMappings.global": {
    "<cr>": ["expandable?", "expand", "open"],
    "v":"open:vsplit",
    "L":"copyRelativeFilepath"
  },
  "explorer.icon.customIcons": {
    "icons": {
      "file":{
        "code":" ",
        "color":"#ffffff"
      }
    }
  },
 "coc.preferences.formatOnSaveFiletypes": ["javascript", "typescript","javascriptreact","typescriptreact","python","css","scss","html"],
  "eslint.enable": true,
  "eslint.alwaysShowStatus":true,
  "prettier.semi": true,
  "python.linting.pylintEnabled":false,
  "python.formatting.provider":"black",
  "python.linting.lintOnSave":true,
  "tailwindCSS.headwind.runOnSave": true,
  "tailwindCSS.headwind.removeDuplicates": true
}

